{
  "name": "ember-drag-sort",
  "version": "4.0.2",
  "description": "A sortable list component with support for multiple and nested lists.",
  "keywords": [
    "ember-addon",
    "drag n drop",
    "drag-n-drop",
    "drag'n'drop",
    "drag",
    "drop",
    "sort",
    "sorting",
    "component",
    "ember-component",
    "sortable",
    "sortable-list",
    "sortable list"
  ],
  "repository": "https://github.com/adopted-ember-addons/ember-drag-sort",
  "license": "MIT",
  "author": "Andrey Mikhaylov @lolmaus <lolmaus@gmail.com> (http://lolma.us)",
  "typesVersions": {
    "*": {
      "test-support": [
        "declarations/addon-test-support/index.d.ts"
      ],
      "test-support/*": [
        "declarations/addon-test-support/*",
        "declarations/addon-test-support/*/index.d.ts"
      ],
      "*": [
        "declarations/addon/*",
        "declarations/addon/*/index.d.ts"
      ]
    }
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "prepack": "tsc --project tsconfig.declarations.json",
    "postpack": "rimraf declarations",
    "start": "ember serve",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\" --prefixColors auto",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@ember/render-modifiers": "^3.0.0",
    "ember-auto-import": "^2.10.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-element-helper": "^0.8.8",
    "ember-template-imports": "^4.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.4",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^5.3.0",
    "@embroider/macros": "^1.18.1",
    "@embroider/test-setup": "^4.0.0",
    "@eslint/js": "^9.36.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/environment-ember-loose": "^1.5.2",
    "@glint/environment-ember-template-imports": "^1.5.2",
    "@glint/template": "^1.5.2",
    "@tsconfig/ember": "^3.0.11",
    "@types/qunit": "^2.19.13",
    "@types/rsvp": "^4.0.9",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.2.1",
    "ember-cli": "~6.7.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-deploy": "^2.0.0",
    "ember-cli-deploy-build": "^3.0.0",
    "ember-cli-deprecation-workflow": "^3.4.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-page-object": "^2.3.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-composable-helpers": "^5.0.0",
    "ember-concurrency": "^3.1.1",
    "ember-load-initializers": "^3.0.1",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "^6.7.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^6.1.0",
    "ember-truth-helpers": "^4.0.3",
    "ember-try": "^4.0.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-ember": "^12.7.4",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-qunit": "^8.2.5",
    "globals": "^15.15.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-ember-template-tag": "^2.1.0",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.5.0",
    "release-plan": "^0.17.2",
    "rimraf": "^5.0.10",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.7.0",
    "stylelint": "^16.24.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "webpack": "^5.101.3"
  },
  "peerDependencies": {
    "ember-source": ">= 4.0.0"
  },
  "engines": {
    "node": ">= 20"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://adopted-ember-addons.github.io/ember-drag-sort/"
  }
}
