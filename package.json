{
  "name": "ember-drag-sort",
  "version": "3.0.1",
  "description": "A sortable list component with support for multiple and nested lists.",
  "keywords": [
    "ember-addon",
    "drag n drop",
    "drag-n-drop",
    "drag'n'drop",
    "drag",
    "drop",
    "sort",
    "sorting",
    "component",
    "ember-component",
    "sortable",
    "sortable-list",
    "sortable list"
  ],
  "license": "MIT",
  "author": "Andrey Mikhaylov @lolmaus <lolmaus@gmail.com> (http://lolma.us)",
  "contributors": [
    "kaliber5/ember-drag-sort (https://github.com/kaliber5/ember-drag-sort)",
    "kaliber5 (https://kaliber5.de)"
  ],
  "homepage": "https://github.com/adopted-ember-addons/ember-drag-sort",
  "bugs": {
    "url": "https://github.com/adopted-ember-addons/ember-drag-sort/issues"
  },
  "repository": "https://github.com/adopted-ember-addons/ember-drag-sort",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.28.4",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^3.3.1",
    "@embroider/test-setup": "^4.0.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~5.12.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-deploy": "^2.0.0",
    "ember-cli-deploy-build": "^3.0.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-page-object": "^2.3.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-composable-helpers": "^5.0.0",
    "ember-concurrency": "^3.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.2.4",
    "ember-qunit": "^8.1.1",
    "ember-resolver": "^12.0.1",
    "ember-source": "^5.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^6.1.0",
    "ember-truth-helpers": "^4.0.3",
    "ember-try": "^3.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-ember": "^12.7.3",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-qunit": "^8.2.5",
    "loader.js": "^4.7.0",
    "prettier": "^3.6.2",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.5.0",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.7.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.1.0",
    "webpack": "^5.101.3"
  },
  "peerDependencies": {
    "ember-source": ">= 4.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://adopted-ember-addons.github.io/ember-drag-sort/"
  }
}
