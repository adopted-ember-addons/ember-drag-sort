    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^1.6.0",
    "sinon": "^8.0.2",
    "sinon-chai": "^3.3.0",
    "webpack": "^5.101.3"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
