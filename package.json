    "ember-addon",
    "drag n drop",
    "drag-n-drop",
    "drag'n'drop",
    "drag",
    "drop",
    "sort",
    "sorting",
    "component",
    "ember-component",
    "sortable",
    "sortable-list",
    "sortable list"
  ],
  "license": "MIT",
  "author": "Andrey Mikhaylov @lolmaus <lolmaus@gmail.com> (http://lolma.us)",
